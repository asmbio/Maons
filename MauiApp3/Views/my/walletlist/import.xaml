<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ASMB.Views.walletlist.import"
             >

    <!--Title="导入"-->
    <VerticalStackLayout Margin="20,0">
        <Label 
            Text="Welcome to ASMB!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Editor  HeightRequest="280" Margin="0,20,0,0" x:Name="exportedit" AutoSize="TextChanges"  Placeholder="请输入秘钥加密后数据"></Editor>
        
        <Entry Margin="0,10,0,0" x:Name="pwd" IsPassword="True" Placeholder="请输入秘钥密码"></Entry>

        <Button   Margin="0,20,0,0" Text="导入" Clicked="Button_Clicked" ></Button>
        <!--<Label TextColor="Green">1. 导出的数据是加密后的数据，需要密码才能解密</Label>-->
    </VerticalStackLayout>
</ContentView>