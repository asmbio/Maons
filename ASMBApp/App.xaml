<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ASMBApp"
             xmlns:localvm="clr-namespace:ASMBApp.ViewModels"
             x:Class="ASMBApp.App">
    <Application.Resources>
        <ResourceDictionary>
            <!--<localvm:MyWallet x:Key="Mywallet"></localvm:MyWallet>-->
            <localvm:VMlc x:Key="VMlc"></localvm:VMlc>
            <local:NihilConverter x:Key="NihilConverter"></local:NihilConverter>
            <local:NihiltoUintConverter x:Key="NihiltoUintConverter"></local:NihiltoUintConverter>
            <local:AddressConverter x:Key="AddressConverter"></local:AddressConverter>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
